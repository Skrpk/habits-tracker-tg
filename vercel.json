{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/analytics/:userId",
      "destination": "/analytics.html?userId=:userId"
    }
  ],
  "functions": {
    "api/webhook.ts": {
      "maxDuration": 10
    },
    "api/reminders.ts": {
      "maxDuration": 60
    },
    "api/setup-webhook.ts": {
      "maxDuration": 10
    },
    "api/analytics.ts": {
      "maxDuration": 10
    }
  }
}

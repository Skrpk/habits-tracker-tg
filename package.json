{
  "name": "habits-tracker",
  "version": "1.0.0",
  "description": "Telegram bot for tracking habits",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc && rm -f dist/index.js dist/index.d.ts dist/index.js.map && cp -r public/* dist/",
    "dev": "tsx src/index.ts",
    "dev:reminders": "tsx src/api/reminders-server.ts",
    "start": "node dist/index.js",
    "setup-webhook": "tsx scripts/setup-webhook.ts"
  },
  "keywords": ["telegram", "bot", "habits", "tracker"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@vercel/node": "^3.0.0",
    "node-telegram-bot-api": "^0.64.0",
    "redis": "^4.6.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/node-telegram-bot-api": "^0.64.0",
    "@types/redis": "^4.0.11",
    "tsx": "^4.7.0",
    "typescript": "^5.3.3"
  }
}

